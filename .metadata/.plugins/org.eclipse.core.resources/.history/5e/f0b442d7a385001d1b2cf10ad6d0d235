	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Random ran = new Random();
		int[] cnt = new int[6];
		int dice;
		
		for (int i = 0; i < 10000; i++) {
			dice = random.randInt(6) + 1;
		}
	}

